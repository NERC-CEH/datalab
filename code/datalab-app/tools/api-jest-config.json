{
  "rootDir": "..",
  "collectCoverageFrom": [ "src/api/**/*.js" ],
  "setupFilesAfterEnv": [ "<rootDir>/src/setupTests.js" ],
  "testMatch": [ "<rootDir>/src/api/**/?(*.)(spec|test).js" ],
  "testEnvironment": "node",
  "transform": {
    "^.+\\.(js)$": "<rootDir>/node_modules/react-scripts/config/jest/babelTransform.js",
    "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/node_modules/react-scripts/config/jest/fileTransform.js"
  },
  "transformIgnorePatterns": [ "[/\\\\]node_modules[/\\\\].+\\.(js)$" ],
  "moduleFileExtensions": [ "web.js", "js", "json" ]
}
