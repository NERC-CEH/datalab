{
  "name": "datalab-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.0.0",
    "auth0-js": "^9.11.2",
    "axios": "0.19.0",
    "bluebird": "3.5.0",
    "body-parser": "1.19.0",
    "chalk": "2.1.0",
    "connected-react-router": "^6.5.2",
    "convict": "5.0.0",
    "cross-storage": "1.0.0",
    "downshift": "^2.0.10",
    "express": "4.17.1",
    "express-jwt": "5.3.1",
    "graphql": "0.10.3",
    "graphql-server-express": "1.1.2",
    "graphql-tag": "2.4.2",
    "history": "4.6.1",
    "jquery": "^3.2.1",
    "jsonwebtoken": "8.1.1",
    "jwks-rsa": "1.2.0",
    "lodash": "4.17.15",
    "moment": "2.21.0",
    "mongoose": "5.6.6",
    "moxios": "0.4.0",
    "prop-types": "15.5.8",
    "querystring": "0.2.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.1.0",
    "react-router": "^5.0.1",
    "react-router-dom": "^5.0.1",
    "redux": "^4.0.4",
    "redux-form": "^8.2.5",
    "redux-logger": "3.0.1",
    "redux-promise-middleware": "4.2.0",
    "toastr": "2.1.2",
    "type-to-reducer": "1.0.3",
    "universal-cookie": "2.1.0",
    "validate.js": "0.11.1",
    "winston": "2.3.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "axios-mock-adapter": "1.17.0",
    "concurrently": "3.4.0",
    "core-js": "^3.1.4",
    "enzyme": "3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "enzyme-to-json": "3.3.5",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "eslint-watch": "^6.0.0",
    "husky": "0.13.3",
    "jest": "24.7.1",
    "jest-enzyme": "7.0.2",
    "lint-staged": "3.4.1",
    "nodemon": "1.19.1",
    "react-scripts": "^3.0.1",
    "react-test-renderer": "16.8.6",
    "redux-devtools": "^3.5.0",
    "redux-mock-store": "1.2.3",
    "shx": "0.2.2",
    "webpack": "4.29.6",
    "webpack-node-externals": "1.6.0"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "browserslist": [
    "last 1 version",
    "> 1%",
    "maintained node versions",
    "not dead"
  ],
  "scripts": {
    "test": "react-scripts test --env=jsdom",
    "test-api": "yarn test-api-ci --watch",
    "test-app": "yarn test-app-ci --watch",
    "test-api-ci": "jest --config tools/api-jest-config.json",
    "test-app-ci": "jest --config tools/app-jest-config.json",
    "eject": "react-scripts eject",
    "lint": "esw src",
    "lint-api": "esw src/api",
    "lint-app": "esw src/app",
    "precommit": "lint-staged",
    "start": "concurrently -p \"[{name}]\" -n \"API,APP\" -c \"bgBlue.bold,bgMagenta.bold\"  \"npm run api\" \"npm run app\"",
    "api": "nodemon --exec babel-node src/api/api.js",
    "app": "react-scripts start",
    "prebuild": "npm run dist-remove && npm run dist-create",
    "build": "concurrently -p \"[{name}]\" -n \"API,APP\" -c \"bgBlue.bold,bgMagenta.bold\" \"npm run build-api\" \"npm run build-app\"",
    "dist-remove": "shx rm -rf ./build ./dist",
    "dist-create": "shx mkdir -p ./build ./dist/api ./dist/app",
    "build-api": "npm run version-api && babel src/api --out-dir dist/api && babel src/shared --out-dir dist/shared",
    "build-app": "npm run version-app && react-scripts build && shx cp -r ./build/* ./dist/app/",
    "version-api": "bash ../../build/version.sh ../../build/template/version.js.template ./src/api/version.js",
    "version-app": "bash ../../build/version.sh ../../build/template/version.js.template ./src/app/version.js"
  }
}
