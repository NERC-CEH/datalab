{
  "name": "datalab-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "auth0-js": "8.8.0",
    "axios": "0.16.1",
    "bluebird": "3.5.0",
    "body-parser": "1.17.1",
    "convict": "3.0.0",
    "cross-storage": "1.0.0",
    "express": "4.15.2",
    "express-jwt": "5.3.0",
    "graphql": "0.10.3",
    "graphql-server-express": "1.0.0",
    "history": "4.6.1",
    "jwks-rsa": "1.2.0",
    "lodash": "4.17.4",
    "moment": "2.18.1",
    "moxios": "0.4.0",
    "prop-types": "15.5.8",
    "react": "15.5.4",
    "react-dom": "15.5.4",
    "react-redux": "5.0.4",
    "react-router-dom": "4.1.1",
    "react-router-redux": "next",
    "redux": "3.6.0",
    "redux-logger": "3.0.1",
    "redux-promise-middleware": "4.2.0",
    "semantic-ui-css": "2.2.10",
    "semantic-ui-react": "0.70.0",
    "toastr": "2.1.2",
    "type-to-reducer": "1.0.3",
    "universal-cookie": "2.0.8",
    "winston": "2.3.1"
  },
  "devDependencies": {
    "babel-cli": "6.24.1",
    "babel-preset-es2015": "6.24.1",
    "chalk": "1.1.3",
    "concurrently": "3.4.0",
    "enzyme": "2.8.2",
    "enzyme-to-json": "1.5.1",
    "eslint": "3.19.0",
    "eslint-config-airbnb": "15.0.2",
    "eslint-plugin-import": "2.6.1",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "6.10.3",
    "eslint-watch": "3.1.0",
    "husky": "0.13.3",
    "jest-enzyme": "3.4.0",
    "lint-staged": "3.4.1",
    "moxios": "0.4.0",
    "nodemon": "1.11.0",
    "react-scripts": "1.0.10",
    "react-test-renderer": "15.5.4",
    "redux-devtools": "3.4.0",
    "redux-mock-store": "1.2.3",
    "shx": "0.2.2",
    "webpack": "2.6.1",
    "webpack-node-externals": "1.6.0"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "scripts": {
    "test": "react-scripts test --env=jsdom",
    "test-api": "yarn test-api-ci -- --watch",
    "test-app": "yarn test-app-ci -- --watch",
    "test-api-ci": "jest --config tools/api-jest-config.json",
    "test-app-ci": "jest --config tools/app-jest-config.json",
    "eject": "react-scripts eject",
    "lint": "esw src",
    "lint-api": "esw src/api",
    "lint-app": "esw src/app",
    "precommit": "lint-staged",
    "start": "concurrently -p \"[{name}]\" -n \"API,APP\" -c \"bgBlue.bold,bgMagenta.bold\"  \"npm run api\" \"npm run app\"",
    "api": "nodemon --exec babel-node src/api/api.js",
    "app": "react-scripts start",
    "prebuild": "npm run dist-remove && npm run dist-create",
    "build": "concurrently -p \"[{name}]\" -n \"API,APP\" -c \"bgBlue.bold,bgMagenta.bold\" \"npm run build-api\" \"npm run build-app\"",
    "dist-remove": "shx rm -rf ./build ./dist",
    "dist-create": "shx mkdir -p ./build ./dist/api ./dist/app",
    "build-api": "npm run version-api && babel-node tools/build-api.js",
    "build-app": "npm run version-app && react-scripts build && shx cp -r ./build/* ./dist/app/",
    "version-api": "bash ../../build/version.sh ../../build/template/version.js.template ./src/api/version.js",
    "version-app": "bash ../../build/version.sh ../../build/template/version.js.template ./src/app/version.js"
  }
}
